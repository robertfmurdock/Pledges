#include "Pods-PledgesTests-ArmorTestKit.xcconfig"
CONFIGURATION_BUILD_DIR = $PODS_FRAMEWORK_BUILD_PATH
FRAMEWORK_SEARCH_PATHS = "$PODS_FRAMEWORK_BUILD_PATH" ${PODS_PLEDGESTESTS_ARMORTESTKIT_FRAMEWORK_SEARCH_PATHS}
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Private" "${PODS_ROOT}/Headers/Private/ArmorTestKit" "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/ArmorTestKit"
OTHER_LDFLAGS = ${PODS_PLEDGESTESTS_ARMORTESTKIT_OTHER_LDFLAGS} -ObjC
OTHER_SWIFT_FLAGS = "-D COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-PledgesTests
PODS_ROOT = ${SRCROOT}